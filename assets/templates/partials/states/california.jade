.form-group
  label.col-lg-4.control-label( for='inputLastName' ) Last Name
  .col-lg-8
    input#inputLastName.form-control( type='text', placeholder="Last Name", ng-model="user.last_name" )
.enclose-group
  .form-group
    label.col-lg-4.control-label( for='inputCID' ) California GO ID#
    .col-lg-8
      input#inputCID.form-control( type='text', ng-model="state.cid" )
    .col-lg-8.col-lg-push-4 This # is on your fishing/hunting license
  .form-group
    label.center - OR -
  .enclose-group
    .form-group
      label.col-lg-4.control-label( for='inputDLN' ) Drivers License #
      .col-lg-8
        input#inputDLN.form-control( type='text', ng-model="user.drivers_license" )
    .form-group
      label.col-lg-4.control-label( for='inputDLN' ) Drivers License State
      .col-lg-8
        select#select.form-control( ng-model="user.dl_state", ng-options="stateOption for stateOption in stateOptions" )
          option( value="" ) State
